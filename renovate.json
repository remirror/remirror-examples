{
    "extends": [
        "config:base",
        ":semanticCommitTypeAll(chore)"
    ],
    "packageRules": [
        {
            "groupName": "Remirror packages",
            "matchPackagePatterns": [
                ".*remirror.*",
                ".*prosemirror.*"
            ],
            "rangeStrategy": "bump"
        }
    ],
    "rangeStrategy": "replace",
    "prConcurrentLimit": 2,
    "dependencyDashboard": true
}
